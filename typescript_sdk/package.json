    "mint-transfer": "ts-node src/mint-and-transfer.ts",
    "mint-with-helper": "ts-node src/mint-with-helper.ts",
    "mint-transfer-direct": "ts-node src/mint-and-transfer-direct.ts",
    "mint-transfer-multisig": "ts-node src/mint-and-transfer-multisig.ts",
    "mint-multisig": "ts-node src/mint-and-transfer-multisig.ts",
    "mint-multisig-two": "ts-node src/mint-and-transfer-multisig-two-users.ts",
    "update-moment-uri": "ts-node src/update-moment-uri.ts",
    "add-token-property": "ts-node src/add-token-property.ts",
    "update-royalties": "ts-node src/update-royalties.ts",
    "burn-moment-simple": "ts-node src/burn-moment-simple.ts",
    "create-collection": "ts-node src/create-collection.ts",
    "get-collection-supply": "ts-node src/get-collection-supply.ts",
